{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "restapi/**.yml",
          "restapi/**"
        ]
      },
      {
        "files": [
          "articles/**/**.md",
          "articles/**/toc.yml",
          "articles/**/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "media/**",
          "articles/images/**",
          "articles/media/**",
          "articles/**/images/**",
          "articles/**/media/**",
          "templates/media/**",
          "node-modules/font-awesome/css/font-awesome.min.css",
          "node-modules/font-awesome/fonts/*",
          "web.config",
          "resources/docsLogo.svg",
          "resources/favicon.ico"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Articles",
      "_enableSearch": true,
      "_appFooter": "Copyright Â© 2019 Health Catalyst",
      "_appLogoPath": "resources/logo.svg",
      "_appFaviconPath": "resources/favicon.ico",
      "_enableNewTab": true,
      "_disableContribution": true
    },    
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/site"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}