{
  "build": {
    "content": [
      {
        "files": [
          "restapi/**"
        ]
      },
      {
        "files": [
          "articles/**/**.md",
          "articles/**/toc.yml",
          "articles/**/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "readme.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "articles/**/media/**",
          "node-modules/font-awesome/css/font-awesome.min.css",
          "node-modules/font-awesome/fonts/*",
          "templates/site/images/**",
          "web.config",
          "resources/docsLogo.svg",
          "resources/favicon.ico"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_enableSearch": true,
      "_appFooter": "Copyright Â© 2019 Health Catalyst",
      "_appLogoPath": "resources/logo.svg",
      "_appFaviconPath": "resources/favicon.ico",
      "_enableNewTab": true,
      "_disableContribution": true,
      "_homePath": "/"
    },
    "fileMetadata": {
      "_noindex": {
        "articles/includes/*": true,
        "readme.md": true
      }
    },
    "dest": "_site",
    "template": [
      "default",
      "templates/site"
    ],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}